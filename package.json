  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lazylogx.addLazyDebugging",
        "title": "Add lazy debbuging"
      }
    ],
    "keybindings": [
      {
        "command": "lazylogx.addLazyDebugging",
        "mac": "ctrl+cmd+alt+l",
        "key": "ctrl+cmd+alt+l",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "lazylogx.addLazyDebugging",
          "group": "navigation"
        }
      ]
    }
  },
  "repository": {
    "type": "https",
    "url": "https://github.com/stiliajohny/lazylogx"
  },
