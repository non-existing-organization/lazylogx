{
  "name": "lazylogx",
  "displayName": "LazyLogX",
  "author": "stilia.johny",
  "license": "GPL-3.0",
  "publisher": "JohnStilia",
  "description": "A VSCode extension that will make lazy debugging easier",
  "icon": "resources/icon.png",
  "version": "0.2.2",
  "preview": true,
  "sponsor": {
    "url": "https://github.com/sponsors/stiliajohny"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "lazylogx",
    "console.log",
    "console",
    "debugging",
    "debug",
    "log",
    "logging",
    "extension",
    "javascript",
    "typescript",
    "js",
    "ts",
    "JavaScript",
    "TypeScript",
    "Python"
  ],
  "contributors": [
    {
      "name": "stiliajohny",
      "url": "https://github.com/stiliajohny"
    },
    {
      "name": "yezen-alnafei",
      "url": "https://github.com/yezen-alnafei"
    }
  ],
  "main": "./extension.js",
  "activationEvents": [
    "onCommand:lazylogx.addLazyDebugging"
  ],
  "contributes": {
    "commands": [
      {
        "command": "lazylogx.addLazyDebugging",
        "title": "Lazy debugging"
      }
    ],
    "keybindings": [
      {
        "command": "lazylogx.addLazyDebugging",
        "mac": "ctrl+cmd+alt+l",
        "key": "ctrl+cmd+alt+l",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "lazylogx.addLazyDebugging",
          "group": "navigation"
        }
      ]
    }
  },
  "repository": {
    "type": "https",
    "url": "https://github.com/stiliajohny/lazylogx"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
